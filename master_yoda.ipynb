   "source": [
    "# given a sentence, return a sentence with the words reversed"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def master_yoda(text):\n",
    "    splittext = text.split()\n",
    "    splittext.reverse()\n",
    "    return \" \".join(splittext)"
   ]
  },
