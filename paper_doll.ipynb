   "source": [
    "#Given a string, return a string where for every character in the original there are three characters"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def paper_doll(text):\n",
    "  return \"\".join([char*3 for char in text])"
   ]
  },
